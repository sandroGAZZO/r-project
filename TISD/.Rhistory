beta_ridge<-function(X,Y,lambda)
{
if(length(dim(X))==2)
{
X=as.matrix(cbind(rep(1,nrow(X)),X))
}
if(length(dim(X))==0)
{
X=as.matrix(cbind(rep(1,length(X)),X))
}
Y=as.vector(Y)
return(c(solve(t(X)%*%X+ diag(lambda,ncol(X)))%*% t(X)%*% Y))
}
beta_ridge(donnees$T12,donnees$maxO3,0)
